    <item rdf:about="http://doc.ubuntu-fr.org/securiser_les_fichiers_pdf?rev=1323958958&amp;do=diff">
        <dc:format>text/html</dc:format>
        <dc:date>2011-12-15T15:22:38+02:00</dc:date>
        <title>securiser_les_fichiers_pdf</title>
        <link>http://doc.ubuntu-fr.org/securiser_les_fichiers_pdf?rev=1323958958&amp;do=diff</link>
        <description>Scuriser les fichiers pdf de faon extrme et sans mot de passe !


#!/bin/bash
# Scuriser les fichiers pdf

# Nom du programme : securisepdf
# securisepdf doit tre copier dans /usr/local/sbin
# et vous devez le rendre excutable en faisant ceci :
# sudo chmod +x /usr/local/sbin/securisepdf

MessageDAide(){
reset
echo -e '\tUTILISATION DU SCRIPT'
echo -e &quot;\r&quot;
echo 'Ne mettez pas d'\''extension'
echo -e &quot;\r&quot;
echo 'securisepdf Nom_Du_Fichier_Source Nom_Du_Fichier_Cible'
echo -e &quot;\r&quot;
}

